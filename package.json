    "amplitude-js": "^8.5.0",
    "axios": "^0.21.3",
    "axios-mock-adapter": "^1.20.0",
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "chai": "^4.3.4",
    "classnames": "^2.3.1",
    "i18next": "^20.4.0",
    "i18next-browser-languagedetector": "^6.1.2",
    "i18next-xhr-backend": "^3.2.2",
